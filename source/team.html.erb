<div class="team container text-center padded">
  <h1>Our team</h1>
    <ul class="list-inline text-center">
      <% ["emile", "liliya"].each do |member| %>
        <li>
          <%= image_tag "#{member}.jpeg", class: "img-circle avatar-xlarge" %>
          <h3><%= member.capitalize %></h3>
        </li>
      <% end %>
    </ul>
    <h3 class="padded">
        <a href="mailto:contact@airbnb.com">
          <i class="fa fa-envelope"></i> contact@airbnb.com
        </a>
    </h3>
</div>
